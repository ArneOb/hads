doctype html
html
  head
    meta(charset='utf-8')
    title #{title}
    link(rel="shortcut icon", href="/_mdds/assets/favicon.ico")
    each style in styles
      link(href=`/_mdds${style}`, rel='stylesheet', type='text/css')
  body
    include header
    block content
    include footer
    include add
    each script in scripts
      script(src=`/_mdds${script}`)

